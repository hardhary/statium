from util import *
cutoff = 5
pdb = get_pdb_info('data/culled_90/1dsz-B.pdb')
N = len(pdb)
distance_matrix = [[None]*(N-i-1) for i in xrange(N)]
lib_ips = set()
for i in xrange(N):
	for j in xrange(i+1, N):
		result = pdb[i].filteredDistancesTo(pdb[j], cutoff)
#		if result is None:
#			print i,j, result
#		else:
#			print i,j, result.keys()[0]
		if result is not None:
			lib_ips.add((i,j))
			distance_matrix[i][j-i-1] = result

